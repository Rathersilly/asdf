<h1>Sessions#new</h1>
<%= form_with(scope: :session, url: login_path) do |f| %>
  <%# render 'shared/errors', object: f.object %> 

  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Log in" %>

<% end %>
<%= params.inspect %>
<p>Find me in app/views/sessions/new.html.erb</p>
